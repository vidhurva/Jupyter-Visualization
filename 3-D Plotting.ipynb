    "import ipyvolume.pylab as p3\n",
    "import numpy as np\n",
    "\n",
    "fig = p3.figure()\n",
    "x, y, z, u, v, w = np.random.random((6, 300))*2-1\n",
    "q = p3.quiver(x,y,z,u,v,w, color=\"purple\", size=8)\n",
    "#p3.style.use(\"dark\") \n",
    "p3.animation_control(q, interval=200)\n",
    "p3.show()"
